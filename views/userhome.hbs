<section class="portfolio section-padding" id="portfolio">
	<div class="container">
		<div class="row">


			





			<div class="col-md-12 mt-3">
				<div class="section-header text-center pb-3">
					<h2 class="fw-bold">Shope now!!</h2>
					<p class="text-warning fw-bold">vintage Wear<span class="text-dark"><br>
							the complete fashion store for universe
						</span>
					</p>
				</div>
			</div>


			

			{{!-- category --}}
			<div class="dropdown">
				<button class="btn btn-light dropdown-toggle" type="button" id="dropdownMenuButton1"
					data-bs-toggle="dropdown" aria-expanded="false">

					category

				</button>
				<ul class="dropdown-menu" aria-labelledby="dropdownMenuButton1">

					<li class="nav-item"><a class="nav-link" href="/mencategory">Men</a></li>
					<li class="nav-item"><a class="nav-link" href="/womencategory">Women</a></li>
					<li class="nav-item"><a class="nav-link" href="/kidscategory">Kids</a></li>

				</ul>

			</div>
	
	
	
	
	
		</div>
		<div class="row">

			{{#if searchItem}}

			{{#each searchItem}}

			{{#if this.stock}}

			<div class="col-12 col-md-12 col-lg-3 p-3">
				<div class="card text-light text-center bg-white pb-2">
					<div class="card-body text-dark"
						style="background:linear-gradient(135deg, #E3E3E3 0%,#5d6874 100%);">
						{{#if this.currentoffer}}
						<div class="bg-secondary rounded text-white position-absolute start-0 top-0 ml-2 m-4 py-1 px-3">
							<h5>{{this.currentoffer}}% <span class="text-warning">off</span></h5>
						</div>
						{{/if}}
						<div class="img-area mb-4"><a href="/productdetail/{{this._id}}"><img alt="" class="img-fluid"
									src="{{this.image.[0]}}"></a></div>
						<a href="/productdetail/{{this._id}}" class="text-decoration-none text-black">
							<h3 class="card-title fw-bold" style="color:white">{{this.name}}</h3>
						</a>
						{{!-- <h5 class="lead ">{{this.description}}</h5> --}}

						{{#if this.currentoffer}}
						<h5 class="fw-bold " style="background: linear-gradient(90deg, #e3ffe7 0%, #d9e7ff 100%);">
							₹{{this.offerprice}}&nbsp<span style="text-decoration:line-through;"> ₹{{this.price}}</span>
						</h5>
						{{else}}
						<h5 class="fw-bold " style="background: linear-gradient(90deg, #e3ffe7 0%, #d9e7ff 100%);">
							₹{{this.offerprice}}</h5>
						{{/if}}

						<div>

							<a onclick="addToCart('{{this._id}}')"><i class="bi bi-cart text-white text-left"></i></a>
							{{!-- <button class="btn-applycoupon mt-5">Add to Cart</button> --}}
						</div>

					</div>
				</div>
			</div>
			{{/if}}

			{{/each}}

             {{/if}}



			
			{{#if products}}

			{{#each products}}

			{{#if this.stock}}

			<div class="col-12 col-md-12 col-lg-3 p-3">
				<div class="card text-light text-center bg-white pb-2">
					<div class="card-body text-dark"
						style="background:linear-gradient(135deg, #E3E3E3 0%,#5d6874 100%);">
						{{#if this.currentoffer}}
						<div class="bg-secondary rounded text-white position-absolute start-0 top-0 ml-2 m-4 py-1 px-3">
							<h5>{{this.currentoffer}}% <span class="text-warning">off</span></h5>
						</div>
						{{/if}}
						<div class="img-area mb-4"><a href="/productdetail/{{this._id}}"><img alt="" class="img-fluid"
									src="{{this.image.[0]}}"></a></div>
						<a href="/productdetail/{{this._id}}" class="text-decoration-none text-black">
							<h3 class="card-title fw-bold" style="color:white">{{this.name}}</h3>
						</a>
						{{!-- <h5 class="lead ">{{this.description}}</h5> --}}

						{{#if this.currentoffer}}
						<h5 class="fw-bold " style="background: linear-gradient(90deg, #e3ffe7 0%, #d9e7ff 100%);">
							₹{{this.offerprice}}&nbsp<span style="text-decoration:line-through;"> ₹{{this.price}}</span>
						</h5>
						{{else}}
						<h5 class="fw-bold " style="background: linear-gradient(90deg, #e3ffe7 0%, #d9e7ff 100%);">
							₹{{this.offerprice}}</h5>
						{{/if}}

						<div>

							<a onclick="addToCart('{{this._id}}')"><i class="bi bi-cart text-white text-left"></i></a>
							{{!-- <button class="btn-applycoupon mt-5">Add to Cart</button> --}}
						</div>

					</div>
				</div>
			</div>
			{{/if}}

			{{/each}}
		
		     {{/if}}
		
		
